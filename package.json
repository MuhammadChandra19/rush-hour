{
  "name": "rush-hour",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "build:api": "turbo run build -F api...",
    "build:cron": "turbo run build -F cron...",
    "build:consumer": "turbo run build -F consumer...",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "turbo": "^2.3.3",
    "typescript": "5.5.4"
  },
  "packageManager": "pnpm@9.4.0",
  "engines": {
    "node": ">=20"
  }
}